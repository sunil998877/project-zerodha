{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/api/index.js" }
    ],
    "env": {
        "MONGO_URL": "@mongo_url",
        "PORT": "@port"
    }
}
